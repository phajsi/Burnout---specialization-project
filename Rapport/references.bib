@incollection{peteiro_alginate_2018,
	address = {Singapore},
	series = {Springer {Series} in {Biomaterials} {Science} and {Engineering}},
	title = {Alginate {Production} from {Marine} {Macroalgae}, with {Emphasis} on {Kelp} {Farming}},
