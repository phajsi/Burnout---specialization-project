	year = {2018},
	doi = {10.1007/978-981-10-6910-9_2},
	keywords = {Alginate extraction, Alginate production, Alginophytes, Aquaculture, Brown seaweeds, Kelp farming, Marine macroalgae, Seaweed alginate, Seaweed resources},
	pages = {27--66},
}

@article{nasatto_methylcellulose_2015,
